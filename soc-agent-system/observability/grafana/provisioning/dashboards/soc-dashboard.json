          },
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "soc_active_websocket_connections",
          "legendFormat": "Connections",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Threats Requiring Review",
      "description": "Current number of threats flagged for human review. High values may indicate increased threat activity or tuning needed.",
